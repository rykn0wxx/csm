<!doctype html>
<html lang="en" ng-app="mud" ng-controller="MudCtrl as vm" ng-cloak="true">
  <head>
  	<meta charset="UTF-8"/>
  	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
  	<meta name="description" content="rykn0wxx" />
  	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, minimal-ui" />
    <title>Csm</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
    <%= yield :head %>

  </head>

  <body class="rykn0wxx">

    <%= content_tag(:input, nil, :type => 'hidden', :value => controller_name, :id => 'ctrlname') %>

    <div id="wrapper" ng-if="$pageFinishedLoading">

      <!-- header -->
      <%= render 'partials/header' %>
      <!-- header -->

      <!-- sidebar -->
      <!-- sidebar -->


      <div class="content-wrapper">

        <div class="content">
          <%= yield %>
        </div>

      </div>

    </div>

    <div id="preloader" ng-if="!$pageFinishedLoading">
      <div layout="row" layout-sm="column" layout-align="space-around">
        <md-progress-circular class="md-accent md-hue-1" md-diameter="96" md-mode="indeterminate"></md-progress-circular>
      </div>
    </div>

  </body>
</html>
