<% title 'New Dashboard' %>
<h1>New Dashboard</h1>

<%= semantic_form_for @dashboard, builder: LabeledFormBuilder do |f| %>
  <%= f.error_messages %>
  <%= f.inputs do %>
    <%= f.collection_select :client_id, Client.all, :id, :name, {prompt: true}, {class: "form-control"} %>
    <%= f.text_field :name, :class => "form-control" %>
    <%= f.text_field :subdomain, :class => "form-control" %>
  <% end %>
  <%= f.submit  %>
<% end %>
<%= link_to 'Back', dashboards_path %>
