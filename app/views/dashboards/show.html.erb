<% title @dashboard.name %>

<section class="section-content">
  <div class="row">
    <div class="col-lg-12 col-md-12">
      <div class="panel">
        <div class="panel-heading">
          <h3 class="panel-title"><%= @dashboard.name %> Dashboard</h3>
        </div>
        <div class="panel-body">
          <div class="horizontal-scroll table-responsive nested-form">
            <%= render 'formpage', dashboard: @dashboard %>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>

<%= link_to 'Edit', edit_dashboard_path(@dashboard) %> |
<%= link_to 'Back', dashboards_path %>
